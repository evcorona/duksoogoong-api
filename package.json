  "version": "1.0.0",
  "description": "",
  "author": "@veroxcrown",
  "engine": {
    "node": "20.x",
    "pnpm": "8.x"
  },
  "scripts": {
    "start": "node api/index.js",
    "dev": "nodemon api/index.js",
    "format": "prettier --write .",
